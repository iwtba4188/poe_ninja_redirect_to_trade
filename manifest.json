{
    "name": "__MSG_extension_name__",
    "description": "__MSG_extension_description__",
    "version": "1.1.4",
    "manifest_version": 3,
    "minimum_chrome_version": "93",
    "default_locale": "en",
    "icons": {
        "128": "./image/icon/icon.png"
    },
    "action": {
        "default_popup": "./src/popup.html"
    },
    "background": {
        "service_worker": "./src/background.js",
        "type": "module"
    },
    "permissions": [
        "webRequest",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://poe.ninja/builds/*"
    ]
}