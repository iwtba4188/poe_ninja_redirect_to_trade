{
    "name": "__MSG_extension_name__",
    "description": "__MSG_extension_description__",
    "version": "1.0",
    "manifest_version": 3,
    "default_locale": "zh_TW",
    "icons": {
        "128": "./image/icon/icon.png"
    },
    "action": {
        "default_popup": "./src/popup.html"
    },
    "background": {
        "service_worker": "./src/background.js"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "*://*.poe.ninja/builds/*"
    ]
}